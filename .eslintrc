{
  "extends": "maptalks",
  "globals": {
    "$": true,
    "d3": true,
    "topojson": true,
    "subdomains": true,
    "echarts": true,
    "Highcharts": true
  },
  "rules": {
    "strict": 0,
    "comma-spacing": 0,
    "space-before-function-paren": [2, {"anonymous": "never", "named": "never"}],
    "indent": ["error", 2]
  }
}
